        <TextBox Name="otch" HorizontalAlignment="Left" Margin="136,173,0,0" TextWrapping="Wrap" Text="Отчество" VerticalAlignment="Top" Width="113" Height="27"/>
        <TextBox Name="mail" HorizontalAlignment="Left" Margin="136,230,0,0" TextWrapping="Wrap" Text="Почта" VerticalAlignment="Top" Width="113" Height="27"/>
        <TextBox Name="pass" HorizontalAlignment="Left" Margin="136,291,0,0" TextWrapping="Wrap" Text="Пароль" VerticalAlignment="Top" Width="113" Height="27"/>
        <Button x:Name="regButton" Content="Регистрация" HorizontalAlignment="Left" Margin="156,372,0,0" VerticalAlignment="Top" Height="30" Width="72" Click="regButton_Click"/>

    </Grid>
